  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slothsoft/example-javascript-application/issues"
  },
  "homepage": "https://github.com/slothsoft/shera-translator/blob/master/README.md",
  "devDependencies": {
    "qunit": "^2.9.2",
    "uglify-js": "^3.6.0"
  }
}
